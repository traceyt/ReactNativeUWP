'use strict';










var RelayNetworkLayer=require.requireActual('RelayNetworkLayer');

function RelayNetworkLayerMock(){
var networkLayer=new RelayNetworkLayer();

networkLayer.fetchRelayQuery=jest.genMockFunction().mockImplementation(
function(){return new Promise(genMockRequest);});

var requests=networkLayer.fetchRelayQuery.mock.requests=[];

networkLayer.sendMutation=jest.genMockFunction();

return networkLayer;

















function genMockRequest(resolve,reject){
requests.push({
resolve:function(response,error){
resolve({error:error||null,response:response});},

reject:reject});}}




RelayNetworkLayerMock.prototype=RelayNetworkLayer.prototype;

module.exports=RelayNetworkLayerMock;