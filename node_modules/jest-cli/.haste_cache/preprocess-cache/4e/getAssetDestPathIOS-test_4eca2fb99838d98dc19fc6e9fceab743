'use strict';









jest.dontMock('../getAssetDestPathIOS');



describe('getAssetDestPathIOS',function(){
it('should build correct path',function(){
var asset={
name:'icon',
type:'png',
httpServerLocation:'/assets/test'};


expect(require('../getAssetDestPathIOS')(asset,1)).toBe('assets/test/icon.png');});


it('should consider scale',function(){
var asset={
name:'icon',
type:'png',
httpServerLocation:'/assets/test'};


expect(require('../getAssetDestPathIOS')(asset,2)).toBe('assets/test/icon@2x.png');
expect(require('../getAssetDestPathIOS')(asset,3)).toBe('assets/test/icon@3x.png');});});