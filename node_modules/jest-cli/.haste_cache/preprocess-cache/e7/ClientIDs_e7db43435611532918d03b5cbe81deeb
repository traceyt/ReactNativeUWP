var 



















_clientIDs={};

var ClientIDs={
getNewClientID:function(){
var timestamp=Date.now();
var clientID=timestamp+':'+(require('randomInt')(0,4294967295)+1);


_clientIDs[clientID]=true;
return clientID;},


isExistingClientID:function(clientID){
return !!_clientIDs[clientID];}};



module.exports=ClientIDs;