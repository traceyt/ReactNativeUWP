'use strict';













var CRMScheduleTypes=require('keyMirror')({
START_NOW:null,
START_LATER:null});




module.exports=babelHelpers.extends({},
CRMScheduleTypes,{
propTypes:require('React').PropTypes.oneOf(Object.keys(CRMScheduleTypes)),

humanReadable:function(scheduleTypeValue){
switch(scheduleTypeValue){
case CRMScheduleTypes.START_NOW:
return require('fbt')._('__FBT__{"type":"text","texts":["Start Now"],"desc":"Label for a radio button in the Budget and Schedule screen that changes the type of the schedule to be start now. This means that newly created CRM will start right after approved.","project":""}__FBT__');







case CRMScheduleTypes.START_LATER:
return require('fbt')._('__FBT__{"type":"text","texts":["Start Later"],"desc":"Label for a radio button in the Budget and Schedule screen that changes the type of the schedule to be start later. This means that newly created CRM will start from user specified date.","project":""}__FBT__');







default:
!false?require('invariant')(0,'Invalid budget type: '+scheduleTypeValue):undefined;}}});