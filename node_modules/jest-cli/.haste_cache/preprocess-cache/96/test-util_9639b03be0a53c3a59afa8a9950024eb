'use strict';







function stripMeta(node){
delete node.start;
delete node.end;
delete node.leadingComments;
delete node.trailingComments;
delete node.raw;
for(var p in node){
if(node[p]&&typeof node[p]==='object'){
stripMeta(node[p]);}}


return node;}


module.exports={
assertSourceAstEqual:function(input,output){
require('assert').deepEqual(
stripMeta(require('babylon').parse(input)),
stripMeta(require('babylon').parse(output)));}};