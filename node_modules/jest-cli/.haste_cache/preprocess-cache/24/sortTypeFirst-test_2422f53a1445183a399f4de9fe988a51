'use strict';



describe('sortTypeFirst', function () {
  it('considers `__type__` to be the smallest', function () {
    expect(require('sortTypeFirst')('__type__', '_')).toBe(-1);
    expect(require('sortTypeFirst')('_', '__type__')).toBe(1);
    expect(require('sortTypeFirst')('__type__', '__type__')).toBe(0);});


  it('does not compare non-`__type__` strings', function () {
    expect(require('sortTypeFirst')('a', 'b')).toBe(0);
    expect(require('sortTypeFirst')('b', 'a')).toBe(0);
    expect(require('sortTypeFirst')('a', 'a')).toBe(0);});});