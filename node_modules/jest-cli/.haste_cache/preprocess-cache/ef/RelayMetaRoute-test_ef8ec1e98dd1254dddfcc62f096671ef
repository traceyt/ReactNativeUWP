'use strict';



describe('RelayMetaRoute', function () {
  beforeEach(function () {
    jest.resetModuleRegistry();});


  it('is immutable', function () {
    var route = new (require('RelayMetaRoute'))('TestRoute');
    expect(function () {return route.name = 'foo';}).toThrow();});


  it('returns caches objects when using `get`', function () {
    expect(
    require('RelayMetaRoute').get('TestRoute')).
    toBe(require('RelayMetaRoute').get('TestRoute'));

    expect(
    require('RelayMetaRoute').get('TestRoute')).
    not.toBe(require('RelayMetaRoute').get('TestRoute2'));

    expect(
    require('RelayMetaRoute').get('TestRoute2')).
    toBe(require('RelayMetaRoute').get('TestRoute2'));});});