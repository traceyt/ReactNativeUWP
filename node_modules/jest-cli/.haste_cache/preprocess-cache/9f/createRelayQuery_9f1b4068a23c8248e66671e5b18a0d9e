'use strict';



















function createRelayQuery(
node,
variables)
{
!(
typeof variables==='object'&&
variables!=null&&
!Array.isArray(variables))?require('invariant')(0,
'Relay.Query: Expected `variables` to be an object.'):undefined;

return require('RelayQuery').Root.create(
node,
require('RelayMetaRoute').get('$createRelayQuery'),
variables);}



module.exports=createRelayQuery;