function 

























binarySearch(arr,compareFunction){
if(__DEV__){
if(typeof arr.length!=='number'){
throw new TypeError('binarySearch() must be passed array-like object.');}}


var low=0,high=arr.length-1;
while(low<=high){
var middle=Math.floor((high+low)/2);
var result=compareFunction(arr[middle],middle);
if(result===0){
return middle;}

if(result>0){
high=middle-1;}else 
{
low=middle+1;}}


return -1;}


module.exports=binarySearch;