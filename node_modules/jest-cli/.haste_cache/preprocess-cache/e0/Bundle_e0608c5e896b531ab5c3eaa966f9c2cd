function generateSourceMapForVirtualModule(module){

var mappings='AAAA;';

for(var i=1;i<module.code.split('\n').length;i++){
mappings+='AACA;';}


return {
version:3,
sources:[module.sourcePath],
names:[],
mappings:mappings,
file:module.sourcePath,
sourcesContent:[module.sourceCode]};}



