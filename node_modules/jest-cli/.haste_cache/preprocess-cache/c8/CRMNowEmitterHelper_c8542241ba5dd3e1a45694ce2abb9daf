'use strict';
























function makeEmitterFunc(eventType){
return function(){
var args=Array.prototype.slice.call(arguments);
args.unshift(eventType);
this.emit.apply(this,args);};}



module.exports={
mixInEmitterFuncs:function(obj,eventTypes){
for(var eventType in eventTypes){
obj[eventType]=makeEmitterFunc(eventType);}}};