'use strict';









var MIN_AGE=13;
var MAX_AGE=65;
var MIN_CALL_NOW_AGE=18;

function isNumber(value){
return typeof value==='number'&&!isNaN(value);}


var AdsManagerAges={
MIN_AGE:MIN_AGE,
MAX_AGE:MAX_AGE,
MIN_CALL_NOW_AGE:MIN_CALL_NOW_AGE,

humanReadableMinAge:function(minAge){
!(
isNumber(minAge)&&minAge>=MIN_AGE)?require('invariant')(0,'min age '+
minAge+' less than allowable value '+MIN_AGE,
minAge,
MIN_AGE):undefined;

return String(minAge);},


humanReadableMaxAge:function(maxAge){
!(
isNumber(maxAge)&&maxAge<=MAX_AGE)?require('invariant')(0,'max age '+
maxAge+' is larger than allowable value '+MAX_AGE):undefined;

return maxAge<MAX_AGE?String(maxAge):require('fbt')._('__FBT__{"type":"text","texts":["{an age in years, e.g. 65}+"],"desc":"Shorthand text for a label on an age selector that indicates an ad will be targeted to a certain age or higher, e.g. 65+. Space is limited.","project":""}__FBT__',[require('fbt').param(




'an age in years, e.g. 65',
MAX_AGE)]);},






humanReadableRange:function(minAge,maxAge){
!(
isNumber(minAge)&&isNumber(maxAge))?require('invariant')(0,'invalid age range ('+
minAge+', '+maxAge+')'):undefined;


if(maxAge>=MAX_AGE){
return require('fbt')._('__FBT__{"type":"text","texts":["{minimum age in years, e.g. 21}+"],"desc":"Shorthand text that represents the age range of the audience that will see an ad when the advertiser has not specified a maximum age, e.g. 24+. This text appears in the description of an audience in the audience selection screen. Space is limited.","project":""}__FBT__',[require('fbt').param(





'minimum age in years, e.g. 21',
AdsManagerAges.humanReadableMinAge(minAge))]);}





if(maxAge===minAge){
return AdsManagerAges.humanReadableMinAge(minAge);}

return require('fbt')._('__FBT__{"type":"text","texts":["{minimum age}-{maximum age}"],"desc":"Shorthand text that represents the age range of an audience that will see an ad based on the minimum and maximum ages an advertiser selected, e.g. \'24 - 37\' or \'24 - 65+\' (65+ signifies 65 or higher). This text appears in the description of an audience in the audience selection screen. Space is limited.","project":""}__FBT__',[require('fbt').param(






'minimum age',
AdsManagerAges.humanReadableMinAge(minAge)),require('fbt').param(


'maximum age',
AdsManagerAges.humanReadableMaxAge(maxAge))]);}};






module.exports=AdsManagerAges;