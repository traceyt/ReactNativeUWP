'use strict';















var AdgroupEditAction={

editCreativeSpec:function(
adgroupID,
creativeSpec,
onSuccess,
onFailure)
{
!
require('isValidRecordID')(adgroupID)?require('invariant')(0,
'Action taken on unknown adgroup ID: '+adgroupID):undefined;


var mutationFrag=require('GraphQLMutationQueryCreator').createQuery(
'adgroup_edit',
{adgroup:adgroupID});


var mutation=function(RQL_0){return {calls:[{kind:'Call',metadata:{},name:'adgroup_edit',value:{kind:'CallVariable',callVariableName:'input'}}],children:[].concat.apply([],[{fieldName:'client_mutation_id',kind:'Field',metadata:{isGenerated:true,isRequisite:true},type:'ID'},require('Relay').QL.__frag(RQL_0)]),kind:'Mutation',metadata:{inputType:'AdgroupEditData!'},name:'AdgroupEditAction',responseType:'AdgroupEditResponsePayload'};}(


mutationFrag);




var action=new (require('AdsManagerMutationAction'))(
require('MobileAdsManagerLoggerConsts').events.EDIT_ADGROUP_CREATIVE);


action.runServerAction(
mutation,
{
adgroup_id:adgroupID,
creative_spec:creativeSpec},

function(err,response){
if(response&&!err){
onSuccess(response);}else 
{
onFailure(err);}


var id=action.getClientMutationID();
require('defaultRQLMutationCallback')(err,response,mutation,id);});}};





module.exports=AdgroupEditAction;