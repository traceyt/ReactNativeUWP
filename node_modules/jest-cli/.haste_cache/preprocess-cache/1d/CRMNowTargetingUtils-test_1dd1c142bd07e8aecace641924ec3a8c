'use strict';







jest.dontMock('CRMNowTargetingUtils').
dontMock('CRMNowGender');;









var LOCATION_COUNTRY_GB={
country_code:'GB',
key:'GB',
location_type:'COUNTRY',
name:'United Kingdom',
alcohol_min_age:18};


var LOCATION_REGION={
country_code:'US',
key:'3886',
location_type:'REGION',
name:'Texas',
alcohol_min_age:21};


var LOCATION_CITY_CA={
country_code:'CA',
key:'296995',
location_type:'CITY',
name:'Vancouver, British Columbia',
alcohol_min_age:19};


var LOCATION_ZIP={
country_code:'US',
key:'US: 94043',
location_type:'ZIP',
name:'94043',
alcohol_min_age:21};


var LOCATION_CUSTOM={
country_code:'CA',
key:'296995',
location_type:'CUSTOM_LOCATION',
name:'1 hacker way, Menlo Park, CA',
address:'1 hacker way, Menlo Park, CA',
alcohol_min_age:21};


var AUDIENCE_FAN={
name:'',
audienceID:-2,
ageMax:65,
ageMin:13,
gender:'FEMALE',
locations:[
LOCATION_COUNTRY_GB],

interests:[],
type:require('CRMNowAudienceType').INLINE_AUDIENCE};


var AUDIENCE_FAN_AND_FRIENDS={
name:'',
audienceID:-6,
ageMax:65,
ageMin:13,
gender:null,
locations:[
LOCATION_COUNTRY_GB,
LOCATION_REGION,
LOCATION_ZIP],

interests:[],
type:require('CRMNowAudienceType').INLINE_AUDIENCE};


var AUDIENCE_INLINE_CAMPAIGN_AUDIENCE={
name:'inline',
audienceID:-5,
ageMax:65,
ageMin:13,
gender:null,
locations:[
LOCATION_COUNTRY_GB],

type:require('CRMNowAudienceType').INLINE_AUDIENCE};


var AUDIENCE_WITHOUT_INTERESTS={
audienceID:123456,
ageMax:45,
ageMin:13,
gender:null,
locations:[
LOCATION_REGION,
LOCATION_ZIP],

type:require('CRMNowAudienceType').SAVE_AUDIENCE};


var AUDIENCE_WITHOUT_LOCATION={
name:'name',
audienceID:123456,
ageMax:45,
ageMin:13,
gender:null,
locations:[],
needsLocations:true,
type:require('CRMNowAudienceType').SAVE_AUDIENCE};


var AUDIENCE_NO_NEED_LOCATION={
name:'name',
audienceID:123456,
ageMax:45,
ageMin:13,
gender:null,
locations:[],
needsLocations:false,
type:require('CRMNowAudienceType').SAVE_AUDIENCE};


var AUDIENCE_WITH_TOO_MANY_COUNTRIES={
name:'name',
audienceID:123456,
ageMax:45,
ageMin:13,
gender:null,
locations:[
{country_code:'GB'},{country_code:'CA'},{country_code:'CN'},
{country_code:'US'},{country_code:'VN'},{country_code:'FR'},
{country_code:'TW'},{country_code:'HK'},{country_code:'BR'},
{country_code:'TH'},{country_code:'KR'},{country_code:'AU'},
{country_code:'TJ'},{country_code:'TZ'},{country_code:'TL'},
{country_code:'TN'},{country_code:'TV'},{country_code:'KZ'},
{country_code:'RU'},{country_code:'RW'},{country_code:'JM'},
{country_code:'KR'},{country_code:'CY'},{country_code:'KH'},
{country_code:'KG'},{country_code:'DK'},{country_code:'ET'}],

type:require('CRMNowAudienceType').SAVE_AUDIENCE};


var AUDIENCE_CUSTOM={
name:'name',
audienceID:123456,
needsLocations:false,
type:require('CRMNowAudienceType').CUSTOM_AUDIENCE};


var AUDIENCE_LOOKALIKE={
name:'name',
audienceID:123456,
needsLocations:false,
type:require('CRMNowAudienceType').LOOKALIKE_AUDIENCE};


var AUDIENCE_WITHOUT_NAME={
name:'',
audienceID:123456,
ageMax:45,
ageMin:13,
gender:null,
locations:[
LOCATION_REGION,
LOCATION_ZIP],

type:require('CRMNowAudienceType').SAVE_AUDIENCE};


var AUDIENCE_WITH_INTERESTS=babelHelpers.extends({},
AUDIENCE_WITHOUT_INTERESTS,{
interests:[
{
name:'Music'},

{
name:'Dance'}]});




var AUDIENCE_LOCAL_AWARENESS={
name:'',
audienceID:-4,
ageMax:45,
ageMin:13,
gender:null,
locations:[
LOCATION_CUSTOM],

type:require('CRMNowAudienceType').INLINE_AUDIENCE};


var LOCATIONS=[
LOCATION_ZIP,
LOCATION_COUNTRY_GB,
LOCATION_CITY_CA];


var PAGE={
whitelisted_countries:[
'CA'],

blacklisted_countries:[
'GB'],

has_alcohol_content:false};


var PAGE_WITH_ALCOHOL_CONTENT={
whitelisted_countries:[
'CA'],

blacklisted_countries:[
'GB'],

has_alcohol_content:true};


var PAGE_WITH_MIN_AGE_RESTRICTION={
default_ad_target_spec:{
min_age:18}};





describe('CRMNowTargetingUtils',function(){

it('detect blacklisted locations correctly',function(){
expect(require('CRMNowTargetingUtils').isBlacklistedCountry()).toBe(false);
expect(require('CRMNowTargetingUtils').isBlacklistedCountry('CA')).toBe(false);
expect(require('CRMNowTargetingUtils').isBlacklistedCountry(
'CA',
PAGE)).
toBe(false);
expect(require('CRMNowTargetingUtils').isBlacklistedCountry(
'US',
PAGE)).
toBe(true);
expect(require('CRMNowTargetingUtils').isBlacklistedCountry(
'GB',
PAGE)).
toBe(true);

expect(require('CRMNowTargetingUtils').getBlacklistedLocations().length).toBe(0);
expect(require('CRMNowTargetingUtils').getBlacklistedLocations(
LOCATIONS).
length).toBe(0);
expect(require('CRMNowTargetingUtils').getBlacklistedLocations(
LOCATIONS,
PAGE).
length).toBe(2);

expect(require('CRMNowTargetingUtils').getBlacklistedLocationsError()).toBe(null);
expect(require('CRMNowTargetingUtils').getBlacklistedLocationsError(
LOCATIONS)).
toBe(null);
expect(require('CRMNowTargetingUtils').getBlacklistedLocationsError(
LOCATIONS,
PAGE)).
not.toBe(null);});


it('get targeting sentences correctly',function(){
expect(
require('CRMNowTargetingUtils').getTargetingSentences().length).
toBe(0);
expect(
require('CRMNowTargetingUtils').getTargetingSentences(
AUDIENCE_FAN).
length).
toBe(2);
expect(
require('CRMNowTargetingUtils').getTargetingSentences(
AUDIENCE_FAN_AND_FRIENDS).
length).
toBe(2);
expect(
require('CRMNowTargetingUtils').getTargetingSentences(
AUDIENCE_WITHOUT_INTERESTS).
length).
toBe(2);
expect(
require('CRMNowTargetingUtils').getTargetingSentences(
AUDIENCE_WITH_INTERESTS).
length).
toBe(3);});


it('detect min age conflicts with alcohol restriction correctly',function(){
expect(
require('CRMNowTargetingUtils').getMinAgeConflictingWithAlcoholRestrictionError(
13)).

toBe(null);
expect(
require('CRMNowTargetingUtils').getMinAgeConflictingWithAlcoholRestrictionError(
18)).

toBe(null);
expect(
require('CRMNowTargetingUtils').getMinAgeConflictingWithAlcoholRestrictionError(
22,
LOCATIONS,
PAGE_WITH_ALCOHOL_CONTENT)).

toBe(null);
expect(
require('CRMNowTargetingUtils').getMinAgeConflictingWithAlcoholRestrictionError(
18,
LOCATIONS,
PAGE_WITH_ALCOHOL_CONTENT)).

not.toBe(null);
expect(
require('CRMNowTargetingUtils').getMinAgeConflictingWithAlcoholRestrictionError(
18,
LOCATIONS,
PAGE)).

toBe(null);
expect(
require('CRMNowTargetingUtils').getAlcohoMinAgeForPageWithLocations(
LOCATIONS,
PAGE_WITH_ALCOHOL_CONTENT)).

toBe(21);
expect(
require('CRMNowTargetingUtils').getAlcohoMinAgeForPageWithLocations(
LOCATIONS,
PAGE)).

toBe(0);
expect(require('CRMNowTargetingUtils').getAlcoholMinAgeForLocations()).toBe(0);
expect(require('CRMNowTargetingUtils').getAlcoholMinAgeForLocations(
LOCATIONS)).
toBe(21);});


it('test audience name should hide correctly',function(){
expect(require('CRMNowTargetingUtils').audienceNameShouldHide()).toBe(false);
expect(require('CRMNowTargetingUtils').audienceNameShouldHide(
AUDIENCE_INLINE_CAMPAIGN_AUDIENCE)).
toBe(true);
expect(require('CRMNowTargetingUtils').audienceNameShouldHide(
AUDIENCE_FAN)).
toBe(false);});


it('get audience name error correctly',function(){
expect(require('CRMNowTargetingUtils').getAudienceNameError()).toBe(null);
expect(require('CRMNowTargetingUtils').getAudienceNameError(
AUDIENCE_WITHOUT_NAME)).
not.toBe(null);
expect(require('CRMNowTargetingUtils').getAudienceNameError(
AUDIENCE_FAN)).
toBe(null);});


it('get audience country limit error correctly',function(){
expect(require('CRMNowTargetingUtils').getCountriesLimitError()).toBe(null);
expect(require('CRMNowTargetingUtils').getCountriesLimitError(
AUDIENCE_WITH_TOO_MANY_COUNTRIES)).
not.toBe(null);
expect(require('CRMNowTargetingUtils').getCountriesLimitError(
AUDIENCE_FAN)).
toBe(null);});


it('get audience empty location error correctly',function(){
expect(require('CRMNowTargetingUtils').getEmptyLocationsError()).not.toBe(null);
expect(require('CRMNowTargetingUtils').getEmptyLocationsError(
AUDIENCE_WITHOUT_LOCATION.locations)).
not.toBe(null);
expect(require('CRMNowTargetingUtils').getEmptyLocationsError(
AUDIENCE_FAN.locations)).
toBe(null);
expect(require('CRMNowTargetingUtils').getEmptyLocationsErrorForReviewScreen()).
toBeNull();
expect(require('CRMNowTargetingUtils').getEmptyLocationsErrorForReviewScreen()).
toBeNull();
expect(require('CRMNowTargetingUtils').getEmptyLocationsErrorForReviewScreen(
AUDIENCE_NO_NEED_LOCATION)).
toBeNull();
expect(require('CRMNowTargetingUtils').getEmptyLocationsErrorForReviewScreen(
AUDIENCE_CUSTOM)).
toBeNull();
expect(require('CRMNowTargetingUtils').getEmptyLocationsErrorForReviewScreen(
AUDIENCE_WITHOUT_LOCATION)).
not.toBe(null);});


it('get audience errors correctly',function(){
expect(
require('CRMNowTargetingUtils').getAudienceErrors().length).
toBe(0);
expect(
require('CRMNowTargetingUtils').getAudienceErrors(
AUDIENCE_NO_NEED_LOCATION).
length).
toBe(0);
expect(
require('CRMNowTargetingUtils').getAudienceErrors(
AUDIENCE_WITHOUT_LOCATION).
length).
toBe(1);
expect(
require('CRMNowTargetingUtils').getAudienceErrors(
AUDIENCE_WITH_TOO_MANY_COUNTRIES).
length).
toBe(1);
expect(
require('CRMNowTargetingUtils').getAudienceErrors(
AUDIENCE_WITHOUT_NAME).
length).
toBe(0);
expect(
require('CRMNowTargetingUtils').getAudienceErrors(
AUDIENCE_INLINE_CAMPAIGN_AUDIENCE).
length).
toBe(0);});


it('get min age restrction from page setting error correctly',function(){
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromPageSettingError()).
toBe(null);
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromPageSettingError(
AUDIENCE_FAN,
0)).
toBe(null);
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromPageSettingError(
AUDIENCE_FAN,
13)).
toBe(null);
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromPageSettingError(
AUDIENCE_FAN,
18)).
not.toBe(null);});


it('get min age restrction from call now cta error correctly',function(){
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromCallNowCTAError()).
toBe(null);
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromCallNowCTAError(
AUDIENCE_LOOKALIKE,
18)).
toBe(null);
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromCallNowCTAError(
AUDIENCE_FAN,
13)).
toBe(null);
expect(require('CRMNowTargetingUtils').getAgeRestrictionFromCallNowCTAError(
AUDIENCE_FAN,
18)).
not.toBe(null);});


it('test audience is custom audience correctly',function(){
expect(require('CRMNowTargetingUtils').isCustomAudience()).toBe(false);
expect(require('CRMNowTargetingUtils').isCustomAudience(
AUDIENCE_LOOKALIKE)).
toBe(true);
expect(require('CRMNowTargetingUtils').isCustomAudience(
AUDIENCE_CUSTOM)).
toBe(true);
expect(require('CRMNowTargetingUtils').isCustomAudience(
AUDIENCE_FAN)).
toBe(false);});


it('get min age from page correctly',function(){
expect(require('CRMNowTargetingUtils').getMinAgeFromPage()).toBe(13);
expect(require('CRMNowTargetingUtils').getMinAgeFromPage(
PAGE)).
toBe(13);
expect(require('CRMNowTargetingUtils').getMinAgeFromPage(
PAGE_WITH_MIN_AGE_RESTRICTION)).
toBe(18);});


it('get selecting audience errors correctly',function(){
expect(require('CRMNowTargetingUtils').getSelectAudienceErrors().length).toBe(0);

expect(require('CRMNowTargetingUtils').getSelectAudienceErrors(
AUDIENCE_CUSTOM,
PAGE).
length).toBe(0);

expect(require('CRMNowTargetingUtils').getSelectAudienceErrors(
AUDIENCE_FAN_AND_FRIENDS,
PAGE).
length).toBe(1);

expect(require('CRMNowTargetingUtils').getSelectAudienceErrors(
AUDIENCE_FAN,
PAGE_WITH_MIN_AGE_RESTRICTION).
length).toBe(1);

expect(require('CRMNowTargetingUtils').getSelectAudienceErrors(
AUDIENCE_FAN,
PAGE_WITH_ALCOHOL_CONTENT).
length).toBe(2);});


it('get targeting errors correctly',function(){
expect(require('CRMNowTargetingUtils').getTargetingErrors().length).toBe(0);

expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_CUSTOM,
PAGE).
length).toBe(0);


expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_NO_NEED_LOCATION,
null).
length).toBe(0);
expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_WITHOUT_LOCATION,
null).
length).toBe(1);
expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_WITH_TOO_MANY_COUNTRIES,
null).
length).toBe(1);
expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_WITHOUT_NAME,
null).
length).toBe(1);
expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_INLINE_CAMPAIGN_AUDIENCE,
null).
length).toBe(0);


expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_FAN_AND_FRIENDS,
PAGE).
length).toBe(1);
expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_FAN,
PAGE_WITH_MIN_AGE_RESTRICTION).
length).toBe(1);
expect(require('CRMNowTargetingUtils').getTargetingErrors(
AUDIENCE_FAN,
PAGE_WITH_ALCOHOL_CONTENT).
length).toBe(2);});


it('get targeting error for review screen correctly',function(){
expect(require('CRMNowTargetingUtils').getTargetingErrorsForReviewScreen().
length).toBe(0);

expect(require('CRMNowTargetingUtils').getTargetingErrorsForReviewScreen(
AUDIENCE_NO_NEED_LOCATION,
13).
length).toBe(0);
expect(require('CRMNowTargetingUtils').getTargetingErrorsForReviewScreen(
AUDIENCE_CUSTOM,
13).
length).toBe(0);
expect(require('CRMNowTargetingUtils').getTargetingErrorsForReviewScreen(
AUDIENCE_WITHOUT_LOCATION,
13).
length).toBe(1);


expect(require('CRMNowTargetingUtils').getTargetingErrorsForReviewScreen(
AUDIENCE_FAN,
18).
length).toBe(1);});


it('get auto generated audience name correctly',function(){
require('CRMNowLocations').humanReadableLocationsSnippet.mockImplementation(
function(locations){
var results=locations.map(function(location){return location.name;});
return require('intlList')(results,require('intlList').CONJUNCTIONS.NONE);});


require('CRMNowGender').humanReadableSymbol=jest.genMockFn().mockImplementation(
function(gender){
if(!gender){
return 'All';}

return gender;});


require('CRMNowAges').humanReadableRange=jest.genMockFn().mockImplementation(
function(ageMin,ageMax){
return ageMin+'-'+ageMax;});



var fbt=require('fbt');
fbt.replaceParams=true;
fbt.jsonEncode=false;
expect(require('CRMNowTargetingUtils').generateNameFromAudience()).toBeNull();
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_CUSTOM)).
toBeNull();
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_WITHOUT_LOCATION)).
toBe(' - All - 13-45');
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_FAN)).
toBe('United Kingdom - FEMALE - 13-65');
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_FAN_AND_FRIENDS)).
toBe('United Kingdom,Texas,94043 - All - 13-65');
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_INLINE_CAMPAIGN_AUDIENCE)).
toBe('United Kingdom - All - 13-65');
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_WITH_INTERESTS)).
toBe('Texas,94043 - All - 13-45');
expect(require('CRMNowTargetingUtils').generateNameFromAudience(
AUDIENCE_LOCAL_AWARENESS)).
toBe('All - 13-45 - 1 hacker way, Menlo Park, CA');});});