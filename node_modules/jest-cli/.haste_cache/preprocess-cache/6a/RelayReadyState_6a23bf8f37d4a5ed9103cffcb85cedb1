'use strict';var 

































RelayReadyState=function(){




function RelayReadyState(onReadyStateChange){babelHelpers.classCallCheck(this,RelayReadyState);
this.$RelayReadyState_onReadyStateChange=onReadyStateChange;
this.$RelayReadyState_readyState={
aborted:false,
done:false,
error:null,
ready:false,
stale:false};

this.$RelayReadyState_scheduled=false;}babelHelpers.createClass(RelayReadyState,[{key:'update',value:function update(


nextReadyState){var _this=this;
var prevReadyState=this.$RelayReadyState_readyState;
if(prevReadyState.aborted){
return;}

if(prevReadyState.done||prevReadyState.error){
if(!nextReadyState.aborted){
require('warning')(
false,
'RelayReadyState: Invalid state change from `%s` to `%s`.',
JSON.stringify(prevReadyState),
JSON.stringify(nextReadyState));}


return;}

this.$RelayReadyState_readyState=babelHelpers.extends({},
prevReadyState,
nextReadyState);

if(this.$RelayReadyState_scheduled){
return;}

this.$RelayReadyState_scheduled=true;
require('resolveImmediate')(function(){
_this.$RelayReadyState_scheduled=false;
_this.$RelayReadyState_onReadyStateChange(_this.$RelayReadyState_readyState);});}}]);return RelayReadyState;}();




module.exports=RelayReadyState;