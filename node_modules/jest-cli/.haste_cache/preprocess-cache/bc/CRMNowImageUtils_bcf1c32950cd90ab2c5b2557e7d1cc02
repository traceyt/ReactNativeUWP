'use strict';











var CRMNowImageUtils={

FULL_IMAGE_SIZE:1000000,

INSTAGRAM_FULL_IMAGE_SIZE:1080,

ImageSourceCategories:require('keyMirror')({
CAMERA_ROLL:null,
PAGE_POST:null,
IMAGE_LIBRARY:null,
INSTAGRAM:null}),


isImageLocal:function(uri){
if(!uri||uri.indexOf('http')===0){
return false;}

return true;},


isValidImageSource:function(image){
return !!(image&&image.uri);}};



module.exports=CRMNowImageUtils;