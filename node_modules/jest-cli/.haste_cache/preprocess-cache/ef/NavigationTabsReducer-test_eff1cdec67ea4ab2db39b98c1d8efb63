'use strict';











jest.
dontMock('NavigationTabsReducer').
dontMock('NavigationFindReducer').
dontMock('NavigationStateUtils');var _NavigationTabsReduce=require('NavigationTabsReducer');var 




JumpToAction=_NavigationTabsReduce.JumpToAction;


describe('NavigationTabsReducer',function(){

it('handles JumpTo with index',function(){
var reducer=require('NavigationTabsReducer')({
tabReducers:[
function(tabState,action){return tabState||'a';},
function(tabState,action){return tabState||'b';},
function(tabState,action){return tabState||'c';}],

initialIndex:1});


var navState=reducer();

expect(navState.children[0]).toBe('a');
expect(navState.children[1]).toBe('b');
expect(navState.children[2]).toBe('c');
expect(navState.children.length).toBe(3);
expect(navState.index).toBe(1);

navState=reducer(
navState,
JumpToAction(2));


expect(navState.children[0]).toEqual('a');
expect(navState.children[1]).toEqual('b');
expect(navState.children[2]).toEqual('c');
expect(navState.children.length).toBe(3);
expect(navState.index).toBe(2);});});