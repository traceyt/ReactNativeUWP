'use strict';











var defaultSchemes=require('createObjectFrom')([
'fb',
'fb-ama',
'fb-messenger',
'fbcf',
'fbconnect',
'fbmobilehome',
'fbrpc',
'file',
'ftp',
'http',
'https',
'mailto',
'ms-app',
'itms',
'itms-apps',
'itms-services',
'market',
'svn+ssh',
'fbstaging',
'tel',
'sms',
'pebblejs',
'react']);


var URISchemes={





isAllowed:function(schema){
if(!schema){
return true;}

return defaultSchemes.hasOwnProperty(schema.toLowerCase());}};



module.exports=URISchemes;