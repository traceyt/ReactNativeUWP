'use strict';











jest.
dontMock('NavigationFindReducer');



describe('NavigationFindReducer',function(){

it('handles basic find reducing with strings',function(){
var reducer=require('NavigationFindReducer')([
function(s){return s;},
function(s){return s+'_yes';},
function(s){return 'nope';}]);

var route=reducer('input');
expect(route).toBe('input_yes');

reducer=require('NavigationFindReducer')([
function(s,action){return s;},
function(s,action){return 'origRoute';},
function(s,action){return 'firstChangedState';}]);

route=reducer('origRoute','action1');
expect(route).toBe('firstChangedState');

reducer=require('NavigationFindReducer')([
function(s,action){return s;},
function(s,action){return action;}]);

route=reducer('inputState','action2');
expect(route).toBe('action2');});});