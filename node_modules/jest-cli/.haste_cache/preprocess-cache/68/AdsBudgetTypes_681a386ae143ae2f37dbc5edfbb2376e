'use strict';













var AdsBudgetTypes=require('keyMirror')({
DAILY:null,
LIFETIME:null});




module.exports=babelHelpers.extends({},
AdsBudgetTypes,{
propTypes:require('React').PropTypes.oneOf(Object.keys(AdsBudgetTypes)),

humanReadable:function(budgetTypeValue){
switch(budgetTypeValue){
case AdsBudgetTypes.DAILY:
return require('fbt')._('__FBT__{"type":"text","texts":["Per Day"],"desc":"Label for a radio button in the Budget and Schedule screen that changes the type of the budget to be daily. This means that the budget amount entered in the input below will be the budget that the ad set can spend each day. The radio button group has one other button for a lifetime budget.","project":""}__FBT__');









case AdsBudgetTypes.LIFETIME:
return require('fbt')._('__FBT__{"type":"text","texts":["Lifetime"],"desc":"Label for a radio button in the Budget and Schedule screen that changes the type of the budget to be lifetime. This means that the budget amount entered in the input below will be the budget that the ad set can spend over its entire duration. The radio button group has one other button for a daily budget. Space is limited.","project":""}__FBT__');










default:
!false?require('invariant')(0,'Invalid budget type: '+budgetTypeValue):undefined;}}});