'use strict';









jest.dontMock('../findXcodeProject');



describe('findXcodeProject',function(){
it('should find *.xcodeproj file',function(){
expect(require('../findXcodeProject')([
'.DS_Store',
'AwesomeApp',
'AwesomeApp.xcodeproj',
'AwesomeAppTests',
'PodFile',
'Podfile.lock',
'Pods'])).
toEqual({
name:'AwesomeApp.xcodeproj',
isWorkspace:false});});



it('should prefer *.xcworkspace',function(){
expect(require('../findXcodeProject')([
'.DS_Store',
'AwesomeApp',
'AwesomeApp.xcodeproj',
'AwesomeApp.xcworkspace',
'AwesomeAppTests',
'PodFile',
'Podfile.lock',
'Pods'])).
toEqual({
name:'AwesomeApp.xcworkspace',
isWorkspace:true});});



it('should return null if nothing found',function(){
expect(require('../findXcodeProject')([
'.DS_Store',
'AwesomeApp',
'AwesomeAppTests',
'PodFile',
'Podfile.lock',
'Pods'])).
toEqual(null);});});