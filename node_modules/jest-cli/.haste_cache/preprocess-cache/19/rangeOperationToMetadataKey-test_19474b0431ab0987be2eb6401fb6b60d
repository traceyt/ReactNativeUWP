'use strict';



describe('rangeOperationToMetadataKey', function () {
  it('maps from developer-friendly name to internal metadata key name', function () {
    expect(require('rangeOperationToMetadataKey')).toEqual({ 
      append: '__rangeOperationAppend__', 
      prepend: '__rangeOperationPrepend__', 
      remove: '__rangeOperationRemove__' });});



  it('is frozen', function () {
    expect(Object.isFrozen(require('rangeOperationToMetadataKey'))).toBe(true);});});