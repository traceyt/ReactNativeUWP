'use strict';
















function printRelayFBQueryCallValue(value){
if(value==null){
return '';}

if(Array.isArray(value)){
return value.map(printRelayFBQueryCallValue).join(',');}


if(value===''){
return ' ';}else 
if(value instanceof require('RelayVariable')){
return '<'+value.getName()+'>';}

if(typeof value!=='string'){
value=JSON.stringify(value);}

value=value.replace(/[)(}{><,.\\]/g,'\\$&');

if(/ $/.test(value)){
value+=' ';}

return value.replace(/^( *)(.*?)( *)$/,function(_,prefix,body,suffix){return (
'\\ '.repeat(prefix.length)+
body+
'\\ '.repeat(suffix.length));});}



module.exports=printRelayFBQueryCallValue;